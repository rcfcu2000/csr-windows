<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="clr-namespace:csr_windows.Resources.Converters">

    <converter:DPIConverter x:Key="DPIConverter" />
    <converter:ChatIdentityEnumConverter
        x:Key="ChatIdentityEnumConverter"
        EqualsVisibility="Visible"
        NotEqualsVisibility="Collapsed" />
    <ResourceDictionary.MergedDictionaries>

        <ResourceDictionary Source="../Theme/Color.xaml" />
        <ResourceDictionary Source="../Theme/Theme.xaml" />
        <ResourceDictionary Source="../Theme/CommonTheme.xaml" />

        <ResourceDictionary Source="pack://application:,,,/HandyControl;component/Themes/Theme.xaml" />
        <ResourceDictionary Source="/csr-windows.Resources;component/Images/ImageManagers/ImageManagerAllDictionary.xaml" />


        <ResourceDictionary Source="ShopStyle/ProductControlStyle.xaml" />
        <ResourceDictionary Source="MenuStyles/MenuItemStyles.xaml" />
        <ResourceDictionary Source="PoptipStyles/PoptipStyle.xaml" />
        <ResourceDictionary Source="ButtonStyles/ButtonStyle.xaml" />
        <ResourceDictionary Source="ToggleButtonStyles/ToggleButtonStyle.xaml" />
        <ResourceDictionary Source="TextBoxStyles/TextBoxStyle.xaml" />
        <ResourceDictionary Source="WindowStyles/WindowStyle.xaml" />
    </ResourceDictionary.MergedDictionaries>

</ResourceDictionary>